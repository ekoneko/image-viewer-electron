{
  "name": "browser",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "author": "ekoneko <arzusyume@gmail.com>",
  "scripts": {
    "start": "parcel src/index.html --no-hmr",
    "build": "npm run clear && npm run build:web && npm run build:preload",
    "clear": "rm -rf dist",
    "build:web": "parcel build src/index.html --no-source-maps --public-url=.",
    "build:preload": "parcel build src/preload.ts --no-source-maps --target electron --public-url=.",
    "release": "cp -R dist/* ../../resources"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.3"
  },
  "alias": {
    "electron": null
  }
}
